<!-- $Id$ -->
<Folder>
   <FolderName>MPXHit</FolderName>
   <FolderTitle>MPXHit</FolderTitle>
   <Affiliation>GMC</Affiliation>
   <ArraySize>variable</ArraySize>
   <Author>
      <AuthorName>Claudio Chiri</AuthorName>
   </Author>
   <FolderVersion>1</FolderVersion>
   <FolderDescription>Folder class for MoDuPixel hit</FolderDescription>
   <NoResetByFramework>false</NoResetByFramework>
   <NoResetModifiedByFramework>false</NoResetModifiedByFramework>
   <Field>
      <FieldName>fNrMPX</FieldName>
      <FieldType>Int_t</FieldType>
      <FieldComment>Modupixel number: 0-Up; 1-down</FieldComment>
      <FieldInitialization>-999</FieldInitialization>
   </Field>
   <Field>
      <FieldName>fTrkIDofHit</FieldName>
      <FieldType>Int_t</FieldType>
      <FieldComment>Track ID for hit</FieldComment>
      <FieldInitialization>-999</FieldInitialization>
   </Field>
   <Field>
      <FieldName>fTotalEnergyLoss</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldComment>Energy deposit</FieldComment>
      <FieldInitialization>-999.</FieldInitialization>
   </Field>
   <Field>
      <FieldName>fT0</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldComment>Track T0 </FieldComment>
      <FieldInitialization>-999.</FieldInitialization>      
   </Field>
   <Field>
      <FieldName>fToF</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldComment>Global Time for hit</FieldComment>
      <FieldInitialization>-999.</FieldInitialization>      
   </Field>
   <Field>
      <FieldName>fxMC</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>
      <FieldComment>MCx coord</FieldComment>
   </Field>
   <Field>
      <FieldName>fyMC</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>
      <FieldComment>MC y coord</FieldComment>
   </Field>
   <Field>
      <FieldName>fzMC</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>
      <FieldComment>MCz coord</FieldComment>
   </Field>
   <Field>
      <FieldName>fnrXpixel</FieldName>
      <FieldType>Int_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>
      <FieldComment>pixel number along x direction</FieldComment>
   </Field>
   <Field>
      <FieldName>fnrYpixel</FieldName>
      <FieldType>Int_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>
      <FieldComment>pixel number along y direction</FieldComment>
   </Field>
   <Field>
      <FieldName>fzMPX</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>
      <FieldComment>Modupix position</FieldComment>
   </Field>
   <Field>
      <FieldName>fsigmapx</FieldName>
      <FieldType>Double_t</FieldType>
      <FieldInitialization>-999.</FieldInitialization>      
      <FieldComment>pixel resolution</FieldComment>
   </Field>
</Folder>
