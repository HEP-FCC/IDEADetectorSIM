<!--$Id:  $-->
<Folder>
   <FolderName>GeantTrack</FolderName>
   <FolderTitle>GeantTrack</FolderTitle>
   <Affiliation>GMC</Affiliation>
   <ArraySize>variable</ArraySize>
   <Author>
      <AuthorName>Claudio Chiri</AuthorName>
   </Author>
   <FolderVersion>1</FolderVersion>
   <FolderDescription>Folder class for Geant track info</FolderDescription>
   <IgnoreTObjectStreamer>true</IgnoreTObjectStreamer> 
   <NoResetByFramework>false</NoResetByFramework>
   <NoResetModifiedByFramework>false</NoResetModifiedByFramework>
   <Include>
     <IncludeName>TVector3.h</IncludeName>
     <IncludeType>global</IncludeType>
   </Include>
   <Include>
     <IncludeName>TString.h</IncludeName>
     <IncludeType>global</IncludeType>
   </Include>
   <Field>
      <FieldName>fTrackID</FieldName>
      <FieldType>Int_t</FieldType>
<!--      <FieldInitialization>-999</FieldInitialization> --> 
      <FieldComment>Track ID</FieldComment>
   </Field>
   <Field>
      <FieldName>fParentID</FieldName>
      <FieldType>Int_t</FieldType>
<!--      <FieldInitialization>-999</FieldInitialization>   --> 
      <FieldComment>Parent ID</FieldComment>
   </Field>
   <Field>
      <FieldName>fParticleName</FieldName>
      <FieldType>TString</FieldType>
<!--      <FieldInitialization>""</FieldInitialization>    --> 
      <FieldComment>Particle name</FieldComment>
   </Field>
   <Field>
      <FieldName>fCharge</FieldName>
      <FieldType>Double_t</FieldType>
<!--      <FieldInitialization>-999.</FieldInitialization>  --> 
      <FieldComment>Particle charge</FieldComment>
   </Field>
   <Field>
      <FieldName>fPDGCode</FieldName>
      <FieldType>Int_t</FieldType>
<!--      <FieldInitialization>999</FieldInitialization>    --> 
      <FieldComment>Particle code in PDG</FieldComment>
   </Field>
   <Field>
      <FieldName>fPosStart</FieldName>
      <FieldType>TVector3</FieldType>
      <FieldInitialization>NULL</FieldInitialization>
      <InheritTObject>true</InheritTObject>
      <FieldComment>Track Initial position</FieldComment>
   </Field>
   <Field>
      <FieldName>fPosEnd</FieldName>
      <FieldType>TVector3</FieldType>
      <FieldInitialization>NULL</FieldInitialization>
      <InheritTObject>true</InheritTObject>
      <FieldComment>Track End position</FieldComment>
   </Field>
   <Field>
      <FieldName>fMomentum</FieldName>
      <FieldType>TVector3</FieldType>
      <FieldInitialization>NULL</FieldInitialization>
      <InheritTObject>true</InheritTObject>
      <FieldComment>Track Momentum</FieldComment>
   </Field>
</Folder>
